      "prepublishOnly": "npm run build",
      "preversion": "npm run check",
      "postversion": "git push && git push --tags"
   },
   "devDependencies": {
      "@rollup/plugin-commonjs": "^22.0.1",
      "@rollup/plugin-node-resolve": "^13.3.0",
