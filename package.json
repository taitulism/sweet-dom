{
   "name": "sweet-dom",
   "description": "A DOM library for developers who don't want a DOM library.",
   "version": "0.0.1",
   "license": "MIT",
   "main": "./src/index.ts",
   "module": "./dist/esm/sweet-dom.esm.js",
   "types": "./dist/esm/sweet-dom.esm.d.ts",
   "files": [
      "dist/esm"
   ],
   "author": "Taitu Lizenbaum <taitu.dev@gmail.com>",
   "homepage": "https://github.com/taitulism/sweet-dom#readme",
   "repository": "github:taitulism/sweet-dom",
   "scripts": {
      "dev": "npm run test:watch",
      "play": "rollup --config rollup.dev.config.js --watch",
      "prebuild": "rm -rf ./dist",
      "build": "rollup --config rollup.prod.config.js",
      "postbuild": "rm -rf ./dist/esm/temp-dts",
      "test": "mocha",
      "test:watch": "mocha --watch-files src,tests --watch",
      "check": "npm run lint && npm run check:types && npm test",
      "check:types": "tsc --noEmit",
      "lint": "eslint ./src ./tests",
      "preversion": "npm run check",
      "version": "npm run build && git add dist",
      "postversion": "git push && git push --tags",
      "prepublishOnly": "npm run check"
   },
   "devDependencies": {
      "@rollup/plugin-commonjs": "22.0.1",
      "@rollup/plugin-node-resolve": "14.1.0",
      "@rollup/plugin-typescript": "8.3.4",
      "@types/chai": "4.3.1",
      "@types/jsdom": "20.0.0",
      "@types/mocha": "10.0.0",
      "@types/sinon": "10.0.13",
      "@typescript-eslint/eslint-plugin": "5.39.0",
      "@typescript-eslint/parser": "5.39.0",
      "chai": "4.3.6",
      "eslint": "8.24.0",
      "jsdom": "20.0.1",
      "mocha": "10.0.0",
      "rollup": "2.79.1",
      "rollup-plugin-dts": "4.2.2",
      "rollup-plugin-terser": "7.0.2",
      "ts-node": "10.9.1",
      "tslib": "2.4.0",
      "typescript": "4.8.4"
   },
   "keywords": [
      "DOM",
      "dom-library"
   ]
}
