      "@types/chai": "^4.3.1",
      "@types/mocha": "^9.1.1",
      "@types/sinon": "^10.0.13",
      "@typescript-eslint/eslint-plugin": "^5.31.0",
      "@typescript-eslint/parser": "^5.31.0",
      "chai": "^4.3.6",
      "eslint": "^8.20.0",
      "jsdom": "^20.0.0",
      "mocha": "^10.0.0",
      "rollup": "^2.77.0",
      "rollup-plugin-dts": "^4.2.2",
      "rollup-plugin-terser": "^7.0.2",
      "ts-node": "^10.9.1",
      "tslib": "^2.4.0",
      "typescript": "^4.7.4"
   }
