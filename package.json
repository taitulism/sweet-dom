   "license": "MIT",
   "private": true,
   "main": "./src/index.ts",
   "browser": "./dist/browser/dom-lib.min.js",
   "module": "./dist/esm/dom-lib.esm.js",
   "types": "./dist/esm/dom-lib.esm.d.ts",
   "files": [
      "dist"
   ],
   "author": "Taitu Lizenbaum <taitu.dev@gmail.com>",
   "homepage": "https://github.com/taitulism/dom-lib#readme",
   "repository": "github:taitulism/dom-lib",
   "scripts": {
      "dev": "rollup --config rollup.dev.config.js --watch",
      "build": "rollup --config rollup.prod.config.js",
      "prebuild": "rm -rf ./dist",
      "postbuild": "rm -rf ./dist/esm/temp-dts",
      "test": "mocha",
